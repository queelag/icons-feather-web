    "@aracna/web": "^1.0.77",
    "@aracna/web-components": "^1.0.92",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "@webcomponents/template-shadowroot": "^0.2.1",
    "dompurify": "^3.1.7",
    "esbuild": "^0.24.0",
    "esbuild-plugin-minify-html-literals": "^2.0.1",
    "glob": "^11.0.0",
    "jsdom": "^25.0.1",
    "lit": "^3.2.1",
    "lit-html": "^3.2.1",
    "prettier": "^3.3.3",
    "ts-lit-plugin": "^2.0.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "engines": {
    "node": ">=16",
